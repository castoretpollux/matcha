#: processes/searchapp/core/forms.py:20
msgid "You must provide a file or text"
msgstr "Vous devez fournir un fichier ou un texte"

#: processes/searchapp/core/views.py:27
msgid "Method must be POST"
msgstr "La méthode doit être POST"

#: processes/searchapp/core/views.py:35
msgid "Method must be either POST or GET"
msgstr "La méthode doit être POST ou GET"

#: processes/searchapp/core/views.py:52 processes/searchapp/core/views.py:88
msgid "query and namespace must be specified"
msgstr "query et namespace doivent être fournis"

#: processes/searchapp/core/views.py:181
msgid "Document imported"
msgstr "Document importé"

#: processes/searchapp/core/views.py:198
msgid "url and namespace must be provided"
msgstr "url et namespace doivent être fournis"

#: processes/searchapp/core/views.py:231
msgid "Crawled website and transformed content to document"
msgstr "Site web crawlé et contenu transformé en document"

#: processes/searchapp/core/views.py:254
#, python-format
msgid "search aggregate result: %s"
msgstr "résultat agrégé de recherche : %s"

#: processes/searchapp/lib/tokenize.py:15
msgid "Overlap must be smaller than chunk size"
msgstr "L'overlap doit être plus petit que la taille de chunk"

#: processes/searchapp/lib/utils.py:82
msgid ""
"Generates a concise english summary of the following text in up to 50 words, "
"gives the summary directly, do not add a commentary or introduction:"
msgstr ""
"Génère un résumé concis en Français du texte suivant en moins de 50 mots, "
"donne le résumé directement, n'ajoute pas de commentaire, d'explications ou "
"d'introduction :"

#: processes/searchapp/lib/utils.py:83
#, python-brace-format
msgid ""
"{preprompt}\n"
"\n"
"{text} \n"
"\n"
"Concise summary:"
msgstr ""
"{preprompt}\n"
"\n"
"{text} \n"
"\n"
"Résumé concis :"
